{
  "name": "network-canvas-interviewer",
  "version": "7.0.0",
  "productName": "Network Canvas Interviewer",
  "description": "A tool for conducting Network Canvas Interviews.",
  "author": "Complex Data Collective",
  "private": true,
  "engines": {
    "node": "18.16.0",
    "npm": "9.5.1"
  },
  "scripts": {
    "dev:ios": "npm run config-cap-dev && npx cap sync ios && npx cap open ios",
    "dev:android": "npm run config-cap-dev && npx cap sync android && npx cap open android",
    "config-cap-dev": "node ./scripts/generateCapacitorConfig.js dev",
    "config-cap-prod": "node ./scripts/generateCapacitorConfig.js",
    "build": "webpack --mode production",
    "start": "webpack serve --mode development",
    "lint:js": "eslint src && eslint public",
    "lint:sass": "sass-lint -vc .sass-lint.yml",
    "lint": "npm run lint:js && npm run lint:sass",
    "test": "node scripts/test.js",
    "test:update-snapshots": "node scripts/test.js --env=jsdom --updateSnapshot",
    "electron": "electron ./www",
    "dist:linux": "npm run build:electron && electron-builder -l",
    "dist:mac": "npm run build:electron && electron-builder -m",
    "dist:win": "npm run build:electron && electron-builder -w",
    "bump": "sh scripts/bump"
  },
  "repository": {
    "type": "git"
  },
  "devDependencies": {
    "@babel/core": "~7.21.8",
    "@babel/preset-env": "~7.21.5",
    "@babel/preset-react": "~7.18.6",
    "@capacitor/cli": "5.0.4",
    "@codaco/eslint-plugin-spellcheck": "0.0.14",
    "@codaco/shared-consts": "~0.0.1-alpha.3",
    "@codaco/ui": "~5.8.3",
    "@faker-js/faker": "~6.0.0-alpha.5",
    "@zippytech/sorty": "^2.0.0",
    "ajv": "^6.5.4",
    "animejs": "^2.2.0",
    "async": "^3.2.0",
    "autoprefixer": "^9.8.5",
    "axios": "^0.21.2",
    "babel-loader": "~9.1.2",
    "chalk": "1.1.3",
    "change-case": "^3.0.1",
    "classnames": "^2.2.6",
    "color": "^2.0.0",
    "compare-versions": "^3.6.0",
    "concaveman": "^1.2.1",
    "connect-history-api-fallback": "1.3.0",
    "connected-react-router": "^6.8.0",
    "cross-env": "^5.2.0",
    "css-loader": "~6.7.4",
    "csvtojson": "^2.0.8",
    "dotenv": "4.0.0",
    "electron": "~9.4.4",
    "electron-builder": "~22.7.0",
    "electron-log": "^4.2.2",
    "electron-notarize": "^1.0.0",
    "electron-winstaller": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "^7.11.1",
    "eventemitter3": "^3.1.0",
    "file-loader": "~6.2.0",
    "framer-motion": "^2.9.1",
    "fs-extra": "3.0.1",
    "fuse.js": "~6.4.6",
    "fuse.js-legacy": "npm:fuse.js@~3.4.5",
    "history": "^4.7.2",
    "html-webpack-plugin": "~5.5.1",
    "jest": "^26.0.1",
    "jest-image-snapshot": "^4.0.2",
    "lodash": "^4.17.11",
    "object-hash": "^1.3.0",
    "postcss-flexbugs-fixes": "~4.2.1",
    "postcss-loader": "~2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.13.1",
    "react-flip-toolkit": "^5.0.5",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-transform-hmr": "^1.0.4",
    "react-transition-group": "^2.5.0",
    "react-virtualized": "~9.22.3",
    "react-virtualized-auto-sizer": "~1.0.5",
    "react-window": "~1.8.6",
    "recompose": "^0.26.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-logger": "^2.7.4",
    "redux-mock-store": "^1.5.3",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.0",
    "rxjs": "^5.5.12",
    "sanitize-filename": "^1.6.3",
    "sass-lint": "^1.12.1",
    "sass-loader": "~13.3.0",
    "style-loader": "~3.3.3",
    "uuid": "^3.4.0",
    "webpack": "~5.84.1",
    "webpack-cli": "~5.1.1",
    "webpack-dev-server": "~4.15.0"
  },
  "dependencies": {
    "@awesome-cordova-plugins/sqlite": "~6.3.0",
    "@babel/runtime": "7.10.1",
    "@capacitor/android": "~5.0.4",
    "@capacitor/app": "5.0.2",
    "@capacitor/browser": "~5.0.2",
    "@capacitor/core": "5.0.4",
    "@capacitor/device": "~5.0.2",
    "@capacitor/filesystem": "~5.0.2",
    "@capacitor/keyboard": "5.0.2",
    "@capacitor/share": "~5.0.2",
    "@capacitor/status-bar": "5.0.2",
    "@capawesome/capacitor-file-picker": "~5.0.0",
    "capacitor-blob-writer": "~1.1.10",
    "cordova-sqlite-storage": "~6.1.0",
    "d3-force": "~3.0.0",
    "dmg-builder": "~23.6.0",
    "electron-devtools-installer": "^3.0.0",
    "fflate": "~0.8.0",
    "jssha": "~3.3.0",
    "sass": "~1.62.1",
    "xcode": "~3.0.1",
    "xmldom": "~0.6.0"
  },
  "homepage": ".",
  "main": "www/electron-starter.js",
  "build": {
    "extends": null,
    "extraResources": [
      "./build-resources/externals/**"
    ],
    "appId": "Network-Canvas-Interviewer-6",
    "afterSign": "./build-resources/scripts/afterSignHook.js",
    "directories": {
      "buildResources": "build-resources",
      "output": "release-builds"
    },
    "win": {
      "target": "nsis",
      "certificateSha1": "8F1243989C0B23B29B50E872282AAB00A4302CCD"
    },
    "mac": {
      "category": "public.app-category.education",
      "hardenedRuntime": true,
      "entitlements": "./build-resources/entitlements.mac.inherit.plist"
    },
    "linux": {
      "maintainer": "Joshua Melville <joshmelville@gmail.com>",
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "complexdatacollective",
        "repo": "interviewer"
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/polyfills.js",
      "<rootDir>/config/jest/enzyme.js",
      "<rootDir>/config/jest/automock.js",
      "<rootDir>/config/jest/matchMedia.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](www|docs|node_modules|scripts|test)[/\\\\]",
      "<rootDir>/src/utils/network-exporters",
      "<rootDir>/src/utils/networkQuery",
      "<rootDir>/integration-tests",
      "<rootDir>/platforms"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "/node_modules/(?!codaco/ui).+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@codaco/ui|d3-force|d3-quadtree|d3-dispatch|d3-timer).+\\.js$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
