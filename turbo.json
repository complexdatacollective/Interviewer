{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["^start"]
    },
    "electron#start": {
      "dependsOn": ["electron#build"],
      "cache": false
    },
    "electron#bundle": {
      "dependsOn": []
    },
    "electron-packager#package": {
      "dependsOn": ["interviewer#build", "electron#bundle"],
      "cache": false
    },
    "electron-packager#package-dir": {
      "dependsOn": ["interviewer#build", "electron#bundle"],
      "cache": false
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
