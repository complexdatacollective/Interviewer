{
  "name": "network-canvas-interviewer",
  "version": "7.0.0-alpha.1",
  "productName": "Network Canvas Interviewer",
  "description": "A tool for conducting Network Canvas interviews.",
  "author": "Complex Data Collective",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development concurrently \"npm:start:web\" \"npm:start:electron\"",
    "start:web": "PORT=4000 REACT_APP_PLATFORM=web craco start",
    "start:electron": "PORT=3000 REACT_APP_PLATFORM=electron BROWSER=none craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,ts,tsx src"
  },
  "overrides": {
    "redux-form": {
      "react-redux": "^8.0.5"
    },
    "react-compound-slider": {
      "react": "^18.2.0"
    }
  },
  "dependencies": {
    "@codaco/network-exporters": "*",
    "@codaco/network-query": "*",
    "@codaco/protocol-utils": "*",
    "@codaco/protocol-validation": "*",
    "@codaco/shared-consts": "*",
    "@codaco/ui": "*",
    "@faker-js/faker": "^7.6.0",
    "@reduxjs/toolkit": "~1.9.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "^9.0.0",
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "compare-versions": "^5.0.1",
    "concaveman": "^1.2.1",
    "concurrently": "^7.6.0",
    "csvtojson": "^2.0.10",
    "d3-force": "^3.0.0",
    "framer-motion": "^9.0.0",
    "fuse.js": "^6.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-virtualized-auto-sizer": "^1.0.7",
    "react-window": "^1.8.8",
    "recompose": "npm:react-recompose@~0.32.0",
    "redux": "^4.2.0",
    "redux-form": "^8.3.9",
    "redux-logger": "^3.0.6",
    "sanitize-filename": "^1.6.3",
    "sass": "^1.56.1",
    "typescript": "^4.9.3",
    "uuid": "~9.0.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "~7.0.3"
  }
}
